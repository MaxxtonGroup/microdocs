{
    "info": {
        "title": "Example application",
        "version": "1.0.0",
        "description": "This is the documentation of an example project for MicroDocs",
        "schemas": ["http"],
        "host": "example.com:8080",
        "basePath": "/",
        "termsOfService": null,
        "licenseName": null,
        "licenseUrl": null,
        "contactName": null,
        "contactUrl": null,
        "contactEmail": null
    },
    "check": {
        "mapping": true,
        "requestParam": true,
        "requestBody": true,
        "responseBody": true
    },
    "links": {
        "basePath": "/",
        "folder": "projects",
        "target": "_blank",
        "package": "com.maxxton",
        "source": "{links.folder}/{group}/{project}/{version}/source/{class}.html"
    },
    "colors": {
        "variable": "#4D90FE",
        "type": "#4CAF50",
        "primary": "#FF9800",
        "desc": "#607D8B",
        "bracket": "#616161"
    },
    "icons": {
        "overview": "bubble_chart",
        "default": "extension",
        "groups": {
            "frontend": "important-devices",
            "services": "hardware:memory"
        }
    },
    "apiblueprint": {
        "format": "1A",
        "groups": [
            "services"
        ],
        "projects": []
    },
    "swagger": {
        "groups": [
            "services"
        ],
        "projects": [],
        "exclude": []
    },
    "cleanup": {
        "storeLimit": 5,
        "historyLimit": 10,
        "fillGaps": true,
        "type": "full"
    }
}
