{
  "global": {},
  "environments": {
    "acceptance": {
      "info": {
        "links": [
          {
            "rel": "repository",
            "href": "https://stash.maxxton.com/projects/${groupCode}/repos/${project.info.title}/browse?at=refs/tags/v${project.info.version}"
          }
        ]
      },
      "components": {
        "{componentName}": {
          "sourceLink": "https://stash.maxxton.com/projects/${groupCode}/repos/${project.info.title}/browse/${sourcePath}${project.components.[componentName].file}?at=refs/tags/v{project.info.version}",
          "methods": {
            "{methodName}": {
              "sourceLink": "${project.components.[componentName].sourceLink}#${project.components.[componentName].methods.[methodName].lineNumber}"
            }
          }
        }
      }
    },
    "development": {
      "info": {
        "links": [
          {
            "rel": "repository",
            "href": "https://stash.maxxton.com/projects/${groupCode}/repos/${project.info.title}/browse"
          }
        ]
      },
      "components": {
        "{componentName}": {
          "sourceLink": "https://stash.maxxton.com/projects/${groupCode}/repos/${project.info.title}/browse/${sourcePath}${project.components.[componentName].file}",
          "methods": {
            "{methodName}": {
              "sourceLink": "${project.components.[componentName].sourceLink}#${project.components.[componentName].methods.[methodName].lineNumber}"
            }
          }
        }
      }
    }
  },
  "groups": {
    "services": {
      "_variables": {
        "sourcePath": "src/main/java/",
        "groupCode": "MXTS"
      }
    },
    "frontend": {
      "_variables": {
        "sourcePath": "",
        "groupCode": "MXTF"
      }
    }
  },
  "projects": {
    "authorization-server": {
      "paths": {
        "/uaa/oauth/token": {
          "post": {
            "consumes": [
              "application/json"
            ],
            "produces": [
              "application/json"
            ],
            "parameters": [
              {
                "in": "query",
                "name": "grant_type",
                "type": "string"
              }
            ],
            "responses": {
              "default": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}