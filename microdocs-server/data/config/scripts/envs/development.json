{
  "info": {
    "links": [
      {
        "rel": "repository",
        "href": "https://stash.maxxton.com/projects/${groupCode}/repos/${project.info.title}/browse"
      }
    ]
  },
  "components": {
    "{componentName}": {
      "sourceLink": "https://stash.maxxton.com/projects/${groupCode}/repos/${project.info.title}/browse/${sourcePath}${scope.file}",
      "methods": {
        "{methodName}": {
          "sourceLink": "${project.components.[componentName].sourceLink}#${scope.lineNumber}"
        }
      }
    }
  },
  "definitions": {
    "{definitionName}": {
      "sourceLink": "https://stash.maxxton.com/projects/${groupCode}/repos/${project.info.title}/browse/${sourcePath}${definitionName|replace \\\\. /}.java"
    }
  }
}